            "post_content": "<!-- wp:paragraph {\"align\":\"center\"} --> <p style=\"text-align:center;\">Wenn Sie einen Tisch reservieren möchten, können Sie uns anrufen oder das untenstehende Formular verwenden.</p> <!-- /wp:paragraph --> <!-- wp:spacer {\"height\":20,\"className\":\"desktop-only\"} --> <div style=\"height:20px;\" aria-hidden=\"true\" class=\"wp-block-spacer desktop-only\"></div> <!-- /wp:spacer --> <!-- wp:separator --> <hr class=\"wp-block-separator\" /> <!-- /wp:separator --> <!-- wp:spacer {\"height\":20} --> <div style=\"height:20px;\" aria-hidden=\"true\" class=\"wp-block-spacer\"></div> <!-- /wp:spacer --> <!-- wp:columns {\"align\":\"wide\",\"className\":\"has-2-columns\"} --> <div class=\"wp-block-columns alignwide has-2-columns\"><!-- wp:column --> <div class=\"wp-block-column\"><!-- wp:heading {\"align\":\"left\",\"level\":3} --> <h3 class=\"has-text-align-left\">Ort</h3> <!-- /wp:heading --> <!-- wp:jetpack/contact-info --> <div class=\"wp-block-jetpack-contact-info\"><!-- wp:jetpack/address {\"address\":\"1 Example Street\",\"city\":\"Anytown\",\"region\":\"NY\",\"postal\":\"10100\"} --> <div class=\"wp-block-jetpack-address\"><div class=\"jetpack-address__address jetpack-address__address1\">Beispielstraße 1</div><div><span class=\"jetpack-address__city\">11234</span> <span class=\"jetpack-address__region\">Irgendwo</span> <span class=\"jetpack-address__postal\">, Deutschland</span></div></div> <!-- /wp:jetpack/address --> <!-- wp:jetpack/email {\"email\":\"mail@example.com\"} --> <div class=\"wp-block-jetpack-email\"><a href=\"mailto:mail@example.com\">mail@beispiel.com</a></div> <!-- /wp:jetpack/email --> <!-- wp:jetpack/phone {\"phone\":\"(555) 555 1234\"} --> <div class=\"wp-block-jetpack-phone\"><a href=\"tel:5555551234\">00112 - 12345</a></div> <!-- /wp:jetpack/phone --> <!-- wp:paragraph --> <p></p> <!-- /wp:paragraph --></div> <!-- /wp:jetpack/contact-info --> <!-- wp:spacer {\"height\":20} --> <div style=\"height:20px;\" aria